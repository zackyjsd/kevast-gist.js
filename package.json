    "@babel/preset-typescript": "^7.1.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.15",
    "babel-loader": "^8.0.4",
    "espower-typescript": "^9.0.1",
    "kevast": "^0.0.9",
    "mocha": "^5.2.0",
    "power-assert": "^1.6.1",
    "ts-loader": "^5.3.1",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
