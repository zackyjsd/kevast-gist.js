{
  "name": "kevast-gist",
  "version": "0.0.1",
  "description": "Gist storage for kevast.js",
  "main": "dist/nodejs/index.js",
  "keywords": [
    "kevast",
    "key",
    "value",
    "key-value",
    "storage",
    "redundancy",
    "gist"
  ],
  "scripts": {
    "lint": "tslint -p tsconfig.json",
